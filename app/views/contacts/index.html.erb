<p id="notice"><%= notice %></p>

<h1>Listing Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Contact name</th>
      <th>Address</th>
      <th>W contact</th>
      <th>Web address</th>
      <th>Email</th>
      <th>Description</th>
      <th>Special</th>
      <th>Fee</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Rating</th>
      <th>Publish</th>
      <th>Publish date</th>
      <th>Hot</th>
      <th>User</th>
      <th>Contact type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.contact_name %></td>
        <td><%= contact.address %></td>
        <td><%= contact.w_contact %></td>
        <td><%= contact.web_address %></td>
        <td><%= contact.email %></td>
        <td><%= contact.description %></td>
        <td><%= contact.special %></td>
        <td><%= contact.fee %></td>
        <td><%= contact.phone1 %></td>
        <td><%= contact.phone2 %></td>
        <td><%= contact.rating %></td>
        <td><%= contact.publish %></td>
        <td><%= contact.publish_date %></td>
        <td><%= contact.hot %></td>
        <td><%= contact.user_id %></td>
        <td><%= contact.contact_type_id %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
